<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="514.0" prefWidth="745.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="grupo1.labtic.ui.restaurants.SolicitarDatos">
    <children>
        <GridPane layoutY="9.0" prefHeight="514.0" prefWidth="745.0" AnchorPane.bottomAnchor="0.0"
                  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="385.0" minWidth="10.0" prefWidth="90.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="420.0" minWidth="10.0" prefWidth="280.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="385.0" minWidth="10.0" prefWidth="62.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="510.0" minWidth="10.0" prefWidth="374.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="175.0" minHeight="0.0" prefHeight="83.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="409.0" minHeight="10.0" prefHeight="392.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <ScrollPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="1">
                    <content>
                        <VBox fx:id="vBox" prefHeight="484.0" prefWidth="344.0" spacing="25.0">
                            <children>
                                <JFXTextField fx:id="nombreRestaurante" focusColor="#493928"
                                              promptText="Nombre del Restaurante" unFocusColor="WHITE"/>
                                <JFXPasswordField fx:id="passNueva" focusColor="#493928" promptText="Nueva contraseña"
                                                  unFocusColor="WHITE"/>
                                <JFXPasswordField focusColor="#493928" promptText="Repetir contraseña"
                                                  unFocusColor="WHITE"/>
                                <HBox minHeight="35.0" prefHeight="35.0" prefWidth="309.0" spacing="20.0">
                                    <children>
                                        <MenuButton fx:id="barriosMenu" contentDisplay="CENTER" mnemonicParsing="false"
                                                    text="Barrio">
                                            <items>
                                                <MenuItem mnemonicParsing="false" text="Action 1"/>
                                                <MenuItem mnemonicParsing="false" text="Action 2"/>
                                            </items>
                                        </MenuButton>
                                        <JFXTextField fx:id="direccionRestaurante" focusColor="#493928"
                                                      promptText="Dirección" unFocusColor="WHITE"/>
                                    </children>
                                    <VBox.margin>
                                        <Insets right="25.0"/>
                                    </VBox.margin>
                                </HBox>
                                <HBox maxHeight="35.0" minHeight="35.0" prefHeight="35.0" prefWidth="200.0"
                                      spacing="20.0">
                                    <children>
                                        <MenuButton fx:id="comidasMenu" mnemonicParsing="false" text="Comidas">
                                            <items>
                                                <MenuItem mnemonicParsing="false" text="Action 1"/>
                                                <MenuItem mnemonicParsing="false" text="Action 2"/>
                                            </items>
                                        </MenuButton>
                                        <MenuButton fx:id="metodosPagoMenu" mnemonicParsing="false"
                                                    text="Métodos de pago">
                                            <items>
                                                <MenuItem mnemonicParsing="false" text="Action 1"/>
                                                <MenuItem mnemonicParsing="false" text="Action 2"/>
                                            </items>
                                        </MenuButton>
                                    </children>
                                </HBox>
                                <JFXTextField fx:id="telefonoRestaurante" focusColor="#493928" promptText="Teléfono"
                                              unFocusColor="WHITE"/>
                                <JFXTextField fx:id="precioMedio" focusColor="#493928" promptText="Precio promedio"
                                              unFocusColor="WHITE"/>
                                <JFXTextField fx:id="webRestaurante" focusColor="#493928" promptText="Sitio web"
                                              unFocusColor="WHITE"/>
                                <JFXTextArea fx:id="descR" focusColor="#493928" promptText="Descripción"
                                             unFocusColor="WHITE"/>
                                <HBox maxHeight="35.0" minHeight="35.0" prefHeight="35.0" prefWidth="200.0">
                                    <children>
                                        <JFXTextField fx:id="hAperturaRestaurante" focusColor="#493928"
                                                      promptText="Abre" unFocusColor="WHITE"/>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text=":"/>
                                        <JFXTextField fx:id="mAperturaRestaurante"/>
                                    </children>
                                </HBox>
                                <HBox prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <JFXTextField fx:id="hCierreRestaurante" focusColor="#493928"
                                                      promptText="Cierra" unFocusColor="WHITE"/>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text=":"/>
                                        <JFXTextField fx:id="mCierreRestaurante" focusColor="#493928"
                                                      unFocusColor="WHITE"/>
                                    </children>
                                </HBox>
                                <HBox prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <JFXTextField fx:id="nMesas" focusColor="#493928" maxWidth="30.0"
                                                      minWidth="30.0" prefWidth="30.0" promptText="n° Mesa"
                                                      unFocusColor="WHITE"/>
                                        <JFXTextField fx:id="nSillas" focusColor="#493928" unFocusColor="WHITE"/>
                                        <JFXButton onAction="#agregarMesa" text="+"/>
                                        <JFXListView fx:id="listMesas"/>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </content>
                </ScrollPane>
            </children>
        </GridPane>
        <ImageView fx:id="imagePortada" fitHeight="150.0" fitWidth="200.0" opacity="0.3" pickOnBounds="true"
                   preserveRatio="true"/>
    </children>
</AnchorPane>
